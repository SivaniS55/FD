{
    "version": 2,
    "builds": [
      {
        "src": "login.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"}
  ]
  }